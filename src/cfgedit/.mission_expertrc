#---------------------------------------------------------------------
# Note
#---------------------------------------------------------------------
#
# FEATURE-TYPE (also used for CONSTRAINT-TYPE below)
#   CBR GLOBAL = 0
#   CBR LOCAL = 1
#   CBR LOCAL + CNP TASK-CONSTRAINT = 2
#   CNP TASK-CONSTRAINT = 3
#   CNP ROBOT-CONSTRAINT = 4
#   CNP RUNTIME-CNP (Mission Manager only) = 5
#   CNP RUNTIME-CNP (Bidder only) = 6
#   CNP RUNTIME-CNP (Mission Manager + Bidder) = 7
#   (Defined in mission_expert_types.h)
#
# FEATURE-NON-INDEX
#   FALSE = 0
#   TRUE = 1
#
# FEATURE-HIDE
#   FALSE = 0
#   TRUE = 1
#
# FEATURE-WEIGHT
#   Choose it from 0.00 to 1.00.
#
# FEATURE-ALLOW-DISABLE (Allow disablement of this feature. Default is
# false. Currently, only supported by global features.)
#    FALSE = 0
#    TRUE = 1
#
# FEATURE-OPTION-TYPE (defined in "mission_expert_types.h")
#   TOGGLE = 0
#   SLIDER1 = 1 ([0.10 - 1.00])
#   SLIDER10 = 2 ([1 - 10])
#   SLIDER100 = 3 ([1 - 100])
#   POLYGONS = 4
#   STRINGS = 5
#
#---------------------------------------------------------------------

#---------------------------------------------------------------------
# Global Features
#---------------------------------------------------------------------

