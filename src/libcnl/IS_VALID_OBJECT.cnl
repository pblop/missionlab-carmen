#include "cnl.inc"

/***********************************************************************/

procedure bool IS_VALID_OBJECT with
   ObjectReading  object;
header
body
   output = object.objcolor != NULL;

   if( debug )
      fprintf(stderr,"IS_VALID_OBJECT(%d): %s\n", robot_id, output?"YES":"NO");
pend


