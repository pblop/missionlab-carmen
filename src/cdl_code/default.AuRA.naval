// TypeInBox Types
//
// The format for TypeInBox is:
// defType <new name> DisplayAs TypeInBox <1: ScrollBox or 0: Non-ScrollBox>
// <1: Show typed contents on the screen or 0: Do not show typed contents on the screen>
// |<description/heading goes to the top of the box>|
defType[AuRA.naval] email_address DisplayAs TypeInBox 0 0|Type-in the email address (if applicable)|;
defType[AuRA.naval] type_alert_subject DisplayAs TypeInBox 0 1|Type-in the headline of this alert|;
defType[AuRA.naval] type_alert_message DisplayAs TypeInBox 1 0|Type-in the alert message|;
defType[AuRA.naval] key_name DisplayAs TypeInBox 0 0|Enter the name of the distributed flag|;
defType[AuRA.naval] type_message DisplayAs TypeInBox 1 0|Type-in a message|;
defType[AuRA.naval] type_notify_message DisplayAs TypeInBox 0 1|Type-in the notify message|;
defType[AuRA.naval] fsa_completion_key DisplayAs TypeInBox 0 0|FSA completion key|;
defType[AuRA.naval] type_map_name DisplayAs TypeInBox 0 1|Type-in the new map name|;
defType[AuRA.naval] type_sub_mission_name DisplayAs TypeInBox 0 1|Type-in the sub-mission name|;
defType[AuRA.naval] cnp_task_constraints_string DisplayAs TypeInBox 1 0|Task Constraints (Format = Name:Type:Value,...)|;
defType[AuRA.naval] cnp_task_id_string DisplayAs TypeInBox 0 1|Task ID (number)|;
defType[AuRA.naval] cnp_task_name_string DisplayAs TypeInBox 0 1|Task Name|;
defType[AuRA.naval] cnp_specific_task_name_string DisplayAs TypeInBox 0 1|Specify the CNP task name (if not using the won task)|;

/********************* Binding points *****************************/
defIBP[AuRA.naval] robot_position sense_location();

defIBP[AuRA.naval] int sense_compass();

defIBP[AuRA.naval] raw_sonar_array sense_raw_sonar();

defIBP[AuRA.naval] obs_array sense_obstacles(
    double max_sensor_range);

defIBP[AuRA.naval] ObjectList sense_objects(
    double max_sensor_range);

defOBP[AuRA.naval] void movement(
    Vector v,
    double base_vel);

defOBP[AuRA.naval] void pantilt(
    Vector look_loc );

defOBP[AuRA.naval] void pantiltzoom(
    Vector look_loc,
    const int zoom );

defRBP[AuRA.naval] vehicle();


defOBP[AuRA.naval] void motivations(
   Vector v );

/******************** sensors ********************/

defSensor[AuRA.naval] binds sense_location robot_position GET_LOCATION();
defSensor[AuRA.naval] binds sense_compass int GET_COMPASS_HEADING();


defSensor[AuRA.naval] binds sense_raw_sonar raw_sonar_array RAW_ULTRASONICS
|Return a list of the raw sonar readings from the robot|
	();

defSensor[AuRA.naval] binds sense_obstacles obs_array DETECT_OBSTACLES
|Return a list of all objects detected within the sonar range (meters)|
      (const double max_sensor_range = {10});

defSensor[AuRA.naval] binds sense_objects ObjectList DETECT_OBJECTS
|Return a list of all objects detected by the vision system|
      (const double max_sensor_range = {100} );

/*defSensor[AuRA.naval] binds video image pulnix_camera();*/


/******************** actuators ********************/

defActuator[AuRA.naval] binds movement void DRIVE(
      Vector v,
      const double max_vel = {0.2},
      const double base_vel = {0.1},
      const double cautious_vel = {0.05},
      const boolean  cautious_mode = {false});

defActuator[AuRA.naval] binds movement void DRIVE_W_SPIN(
      Vector v,
      const double max_vel = {0.2},
      const double base_vel = {0.1},
      const double cautious_vel = {0.05},
      const boolean  cautious_mode = {false});

defActuator[AuRA.naval] binds pantilt void PANTILT(
      Vector look_loc );

defActuator[AuRA.naval] binds pantiltzoom void PANTILTZOOM(
      Vector look_loc,
      const int zoom );


defActuator[AuRA.naval] binds motivations void MOTIVATIONS(
      Vector v);

defActuator[AuRA.naval] binds movement void DRIVE_ROOMBA(
      Vector v,
      const double max_vel = {0.2},
      const double base_vel = {0.1},
      const double cautious_vel = {0.05},
      const boolean  cautious_mode = {false});


/******************** robots ********************/

defRobot[AuRA.naval] binds vehicle MRV2(
    actuator base:DRIVE,
    sensor   xyt:GET_LOCATION,
    sensor   vision_system:DETECT_OBJECTS);

defRobot[AuRA.naval] binds vehicle HUMMER(
    actuator base:DRIVE,
    sensor   xyt:GET_LOCATION,
    sensor   vision_system:DETECT_OBJECTS);

defRobot[AuRA.naval] binds vehicle PIONEERAT(
    actuator base:DRIVE,
    actuator camera:PANTILT,
    sensor   vision_system:DETECT_OBJECTS,
    sensor   sonars:DETECT_OBSTACLES,
    sensor   shaftencoders:GET_LOCATION);

defRobot[AuRA.naval] binds vehicle DEFAULT_ROBOT(
    actuator base:DRIVE,
    actuator camera:PANTILT,
    sensor   vision_system:DETECT_OBJECTS,
    sensor   sonars:DETECT_OBSTACLES,
    sensor   shaftencoders:GET_LOCATION);

defRobot[AuRA.naval] binds vehicle NOMAD200(
    actuator base:DRIVE,
    actuator camera:PANTILT,
    actuator motivations:MOTIVATIONS,
    sensor   vision_system:DETECT_OBJECTS,
    sensor   sonars:DETECT_OBSTACLES,
    sensor   shaftencoders:GET_LOCATION);

/********************* Operators *****************************/

defOp[AuRA.naval] binds FSA Vector FSA FSAstyle(
//    boolean reset_when_regain_scope,
    list [boolean triggers, expression rules, member society]);

defOp[AuRA.naval] binds COOP Vector COOP SELECTstyle(
   list [Vector members, const float weight = {1.0}]
   );

defOp[AuRA.naval] binds MDL_coordinator Vector MDL_coordinator MDLstyle(
   commands command_list,

   Vector moveto,
   Vector teleop,
   Vector occupy,

   boolean moveto_done,
   boolean teleop_done,
   boolean occupy_done
   );

defOp[AuRA.naval] binds QLEARN Vector QLEARN RLstyle(
   list boolean triggers,
   list Vector society,
   list Reinforcer reinforcers,
   //const string QfileName,
   const CNLString_t QfileName,
   const double ActionTimeout,
   const double alpha,
   const double alphaDecay,
   const double random,
   const double randomDecay
);

