# ----------------------------------------------------------------------
#                           FLAGS ETC.
# ----------------------------------------------------------------------

$(shell touch ./cmdli.env) # Real cmdli.env will be generated by 'make depend'
include ./cmdli.env

TARGET = cmdli

# ----------------------------------------------------------------------
#                        COMPILATION RULES
# ----------------------------------------------------------------------

all:
	$(MAKE) $(TARGET)

$(TARGET):
	@ echo "Compiling cmdli: TARGET_OS = $(TARGET_OS)"
	@ ./support/build_sara.sh

clean:
	@ ./support/clean_sara.sh

veryclean: clean

depend:
	@ ./configure

nodepend:
	

# ----------------------------------------------------------------------
#                          DEPENDENCIES
# ----------------------------------------------------------------------
# DO NOT DELETE
