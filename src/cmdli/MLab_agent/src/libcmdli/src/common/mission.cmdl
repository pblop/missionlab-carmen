2. unit robot1 MoveTo location = [PP1a]
   unit robot2 MoveTo location = [PP2a]
   unit robot3 MoveTo location = [PP3a]
3. unit robot1 MoveTo location = [PP1b]
   unit robot2 MoveTo location = [PP2b]
   unit robot3 MoveTo location = [PP3b]
*/
4. unit all MoveTo location = "<0,0>"
5. unit all Stop
6. unit all Exit

