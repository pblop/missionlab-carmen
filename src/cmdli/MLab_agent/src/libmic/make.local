SUBS = common Vector Grid FittedCurve compress TimeOfDay
ifeq ($(TARGET_OS),win32)
   SUBS += zlib-1.2.1
endif
ifeq ($(TARGET_OS),linux)
   SUBS += encryption
endif

ADIR = ../lib/$(TARGET_OS)
ALIB = mic
ARCHIVE = $(ADIR)/lib$(ALIB)$(LIBEXT)

