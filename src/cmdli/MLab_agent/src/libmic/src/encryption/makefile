# ---------------------------------------------------

# Get system wide defaults
include $(SUPPORT_DIR)/make.include
include ../../make.local

# ---------------------------------------------------

CC_SRCS = encryption.cc encryption_evp.cc

# ---------------------------------------------------

INCLUDES = -I../common -I. 

../$(ARCHIVE): $(OBJS)
	$(AR) $(ARFLAGS) $@ $^
	
all: ../$(ARCHIVE)

LLIBS = -L../$(ADIR) -l$(ALIB)

run: test.cc
	$(CC) $(CFLAGS) $(INCLUDES) test.cc $(LDOPTS) $(LDFLAGS) $(LLIBS) $(LIBS) -o test
	./test >& log

test1$(BINEXT): test1$(OBJEXT)
	$(LD) $< $(LDOPTS) $(LDFLAGS) $(LLIBS) $(LIBS) -lcrypto $(EXENAMEFLAG)$@
	$@

test2$(BINEXT): test2$(OBJEXT)
	$(LD) $< $(LDOPTS) $(LDFLAGS) $(LLIBS) $(LIBS) -lcrypto $(EXENAMEFLAG)$@
	$@

doc: FittedCurve.ps

# Get general, invariant targets
include $(SUPPORT_DIR)/make.targets

