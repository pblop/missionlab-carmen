clean:
	rm -f *.o *.obj *.bak core core.* *.exe test *.map *.lst *.ps *.pdb *.output *.idb *.pdb $(TESTS) $(LY_CC) $(LY_H) $(DATAS) $(EXTRA_DELETES)


veryclean: clean
	rm -f build_version build_count
	$(SUPPORT_DIR)/clean_makefile.sh makefile 

