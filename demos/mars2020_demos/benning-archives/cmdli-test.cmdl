MISSION NAME "Joint Demo"

NEW-ROBOT atrvjrClass "atrvjrRobot"
NEW-ROBOT truckClass "truckRobot"
NEW-ROBOT pioneerClass "pioneerRobot"
NEW-ROBOT segwayClass "segwayRobot"

UNIT <gtech-testRobot1> atrvjrClass
UNIT <gtech-testRobot2> atrvjrClass

UNIT <upenn_1> truckClass
UNIT <upenn_2> truckClass
UNIT <upenn_3> truckClass

UNIT <usc_pioneer1> pioneerClass
UNIT <usc_pioneer2> pioneerClass
UNIT <usc_segway> segwayClass

UNIT <north_cache> (gtech-testRobot1 upenn_1 usc_pioneer2)
UNIT <south_cache> (gtech-testRobot2 upenn_2 upenn_3 usc_pioneer1 usc_segway)
UNIT <gtech> (gtech-testRobot1 gtech-testRobot2)

COMMAND LIST:
   unit gtech-testRobot1 MoveTo goalLocation = [GIT-A1]
   unit gtech-testRobot2 MoveTo goalLocation = [GIT-B1]
   unit gtech Synchronize

   unit gtech-testRobot1 MoveTo goalLocation = [GIT-A2]
   unit gtech-testRobot1 MoveTo goalLocation = [GIT-A3]
   unit gtech-testRobot1 Loiter
   unit gtech-testRobot2 MoveTo goalLocation = [GIT-B2]
   unit gtech-testRobot2 MoveTo goalLocation = [GIT-B3]
   unit gtech-testRobot1 Loiter
