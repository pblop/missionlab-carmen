MISSION NAME "sample_indoor_task2"
OVERLAY "marc.ovl"
NEW-ROBOT sample_indoor_task2Robot1 "sample_indoor_task2Robot1" "blue" "" (
 robot_type = HOLONOMIC,
 run_type= SIMULATION)

NEW-ROBOT sample_indoor_task2Robot2 "sample_indoor_task2Robot2" "blue" "" (
 robot_type = HOLONOMIC,
 run_type= SIMULATION)

NEW-ROBOT Enemy "./SentryRobot1" "red" (robot_type = HOLONOMIC, run_type= SIMULATION, extra_robot = true)

UNIT <all> sample_indoor_task2Robot1 sample_indoor_task2Robot2
UNIT <extra> Enemy

set show-trails off
set scale-robots on
set ROBOT-LENGTH 0.5


COMMAND LIST:
 0. UNIT all START StartPlace 1.000000 0.000000
 AND UNIT extra START SentryStartPlace 1.000000 0.000000
